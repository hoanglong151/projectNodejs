html
  head
    meta(name='viewport', content='width=device-width, initial-scale=1')
    link(rel='stylesheet', href='https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css')
    link(rel='stylesheet', href='https://pro.fontawesome.com/releases/v5.10.0/css/all.css', integrity='sha384-AYmEC3Yw5cVb3ZcuHtOA93w35dYTsvhLPVnYs9eStHfGJvOvKxVfELGroGkvsg+p', crossorigin='anonymous')
    link(rel='preconnect', href='https://fonts.googleapis.com')
    link(href='https://meyerweb.com/eric/tools/css/reset/reset200802.css')
    link(rel='preconnect', href='https://fonts.gstatic.com', crossorigin='')
    link(href='https://fonts.googleapis.com/css2?family=Poppins:wght@300;400;600;700;900&display=swap', rel='stylesheet')
    link(rel='preconnect', href='https://fonts.googleapis.com')
    link(rel='preconnect', href='https://fonts.gstatic.com', crossorigin='')
    link(href='https://fonts.googleapis.com/css2?family=Playball&display=swap', rel='stylesheet')
    link(rel='stylesheet' type='text/css' href='./css/index.css')
  header
    .header_block
      .header_main
        .container.header
          a(href='#')
            img.header_logo(src='https://premiumlayers.com/html/royalbakery/images/slider-ic.png')
          ul.header_navbar
            li 
              a(href='#') Category
              ul.header_subnav
                li 
                  a(href='#') bánh kem 
                li 
                  a(href='#') bánh su 
                li 
                  a(href='#') bánh bò
            li 
              a(href='#') Product 
            li 
              a(href='#') About 
            li 
              a(href='#') Contact
          .header_form-search
            form(action="", method="post")
              input(type="text", name="search", placeholder="Nhập từ khóa")
              button.header_btn-search 
                i.fas.fa-search
            button
              i.fas.fa-shopping-cart
            button.menu-mobile
              i.fas.fa-bars

      //- Menu mobile
      .header_menu-mobile
        ul.header_navbar
          li 
            a(href='#') Category
            i.fas.fa-chevron-down.btn-dropdown-menu-mobile
            ul.header_subnav
              li 
                a(href='#') bánh kem 
              li 
                a(href='#') bánh su 
              li 
                a(href='#') bánh bò
          li 
            a(href='#') 
              span Product
          li 
            a(href='#') 
              span About
          li 
            a(href='#') 
              span Contact

  //- Main Carosel
  #carouselExampleCaptions.carousel.slide.carousel-fade(data-bs-ride='carousel')
    .carousel-inner
      .carousel-item.active(data-bs-interval="5000")
        img.thumb-image.d-block.w-100(src='https://premiumlayers.com/html/royalbakery/images/slider-1.jpg', alt='...')
        .carousel-caption.d-md-block
          img(src='https://premiumlayers.com/html/royalbakery/images/slider-ic.png')
          h1 CAKE FOR LATER CAKE 
          h1.carousel-heading AS A WAY OF LIFE
          a(href='#').btn READ MORE
      .carousel-item(data-bs-interval="5000")
        img.thumb-image.d-block.w-100(src='https://premiumlayers.com/html/royalbakery/images/slider-2.jpg', alt='...')
        .carousel-caption.d-md-block
          img(src='https://premiumlayers.com/html/royalbakery/images/slider-ic.png')
          h1 CAKE FOR LATER CAKE
          h1.carousel-heading AS A WAY OF LIFE
          a(href='#').btn READ MORE
      .carousel-item(data-bs-interval="5000")
        img.thumb-image.d-block.w-100(src='https://premiumlayers.com/html/royalbakery/images/slider-3.jpg', alt='...')
        .carousel-caption.d-md-block
          img(src='https://premiumlayers.com/html/royalbakery/images/slider-ic.png')
          h1 CAKE FOR LATER CAKE
          h1.carousel-heading AS A WAY OF LIFE
          a(href='#').btn READ MORE
    a.carousel-control-prev(href='#', data-bs-target='#carouselExampleCaptions', data-bs-slide='prev')
      i.fas.fa-caret-left
      span.visually-hidden Previous
    a.carousel-control-next(href='#', data-bs-target='#carouselExampleCaptions', data-bs-slide='next')
      i.fas.fa-caret-right
      span.visually-hidden Next

  body
    block content
  footer
    .container-fuild.footer-peseudo
      .container.footer-subcribe
        h5.title SUBSCRIBE TO OUR NEWSLETTER
        form(action="")
          .footer-form
            .footer-form_input
              input(type="text", placeholder="Your Email Here")
              i.far.fa-envelope
            button.btn subscribe
    .container-fuild.footer
      .container.footer-list
        .footer-list_item 
          h5.title Hoàng Long
          p.content The day for a three hour tour a three hour tour then one day he was shooting at some food and up through the ground oil.
          a.btn.contact(href='#') contact
        .footer-list_item 
          h5.title ORDER ONLINE
          p.content The day for a three hour tour a three hour tour then one day he was shooting at some food and up through the ground oil.
          a.btn.order-now(href='#') order now
        .footer-list_item 
          h5.title CONTACT US TODAY
          p.content Call Us 666 777 888 OR 111 222 333 Send an Email on contact@cakeshop.com Visit Us 123 Fake Street- London 12358 United Kingdom
          .footer-list_link
            a(href='#')
              i.fab.fa-facebook-f
            a(href='#')
              i.fab.fa-twitter
            a(href='#')
              i.fab.fa-google-plus-g
            a(href='#')
              i.fas.fa-basketball-ball
    .container-fuild.copyrights 
      .container.copyrights_content
        p © Copyrights 2016 Long Hoang. All Rights Reserved
